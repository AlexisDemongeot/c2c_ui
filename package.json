{
  "name": "c2c_ui",
  "description": "https://www.camptocamp.org user interface",
  "version": "7.3.1",
  "author": "Camptocamp community <dev@camptocamp.org>",
  "repository": {
    "type": "git",
    "url": "https://github.com/c2corg/c2c_ui.git"
  },
  "license": "AGPL-3.0-or-later",
  "homepage": "https://www.camptocamp.org",
  "bugs": {
    "url": "https://forum.camptocamp.org/c/site-et-association/v6-suggestions-bugs-et-problemes",
    "email": "dev@camptocamp.org"
  },
  "scripts": {
    "serve": "cross-env BUILD_ENV=local vue-cli-service serve",
    "build": "cross-env BUILD_ENV=camptocamp vue-cli-service build --modern",
    "build:github": "cross-env BUILD_ENV=github vue-cli-service build --modern",
    "lint": "vue-cli-service lint",
    "lint:no-fix": "vue-cli-service lint --no-fix",
    "messages:extract": "node tools/extract-messages.js",
    "messages:push": "curl -L --user api:$TRANSIFEX_TOKEN -F file=\"@src/translations/po/c2corg_ui-client.pot\" -X PUT \"https://www.transifex.com/api/2/project/c2corg_ui/resource/main/content/\"",
    "messages:compile": "node ./tools/compile-messages.js --user api:$TRANSIFEX_TOKEN",
    "generate-icons": "node tools/generate-icons.js",
    "update-c2c-common": "python3 tools/update-c2c-common.py",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.13.0",
    "@fortawesome/fontawesome-svg-core": "1.2.28",
    "@fortawesome/free-brands-svg-icons": "5.13.0",
    "@fortawesome/free-regular-svg-icons": "5.13.0",
    "@fortawesome/free-solid-svg-icons": "5.13.0",
    "@fortawesome/vue-fontawesome": "0.1.9",
    "ag-grid-community": "23.1.0",
    "ag-grid-vue": "23.1.0",
    "axios": "0.19.2",
    "blueimp-load-image": "5.6.0",
    "bulma": "0.8.2",
    "bulma-checkradio": "1.1.1",
    "core-js": "3.6.5",
    "d3": "5.16.0",
    "file-saver": "2.0.2",
    "moment": "2.25.3",
    "ol": "6.3.1",
    "proj4": "2.6.1",
    "vue": "2.6.11",
    "vue-analytics": "5.22.1",
    "vue-awesome-swiper": "3.1.3",
    "vue-infinite-scroll": "2.0.2",
    "vue-multiselect": "2.1.6",
    "vue-property-decorator": "8.4.2",
    "vue-recaptcha": "1.3.0",
    "vue-router": "3.1.6",
    "vue-slider-component": "3.1.2",
    "vuejs-datepicker": "1.6.2"
  },
  "devDependencies": {
    "@vue/babel-preset-app": "4.3.1",
    "@vue/cli-plugin-babel": "4.3.1",
    "@vue/cli-plugin-eslint": "4.3.1",
    "@vue/cli-service": "4.3.1",
    "@vue/eslint-config-standard": "5.1.2",
    "angular-gettext-tools": "2.5.3",
    "babel-eslint": "^10.1.0",
    "cross-env": "7.0.2",
    "eslint": "6.8.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-vue": "6.2.2",
    "husky": "4.2.5",
    "node-sass": "4.14.1",
    "sass-loader": "8.0.2",
    "snyk": "1.319.2",
    "vue-template-compiler": "2.6.11",
    "webpack-bundle-analyzer": "3.7.0",
    "xml2js": "0.4.23"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "cover 95% in FR",
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie <= 10"
  ],
  "snyk": true,
  "husky": {
    "hooks": {
      "pre-commit": "npm run messages:extract && git add src/translations/po/c2corg_ui-client.pot"
    }
  }
}
